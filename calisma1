using System;

namespace ConsoleApp2
{
      class Program
    {
        static void Main(string[] args)
        {
            int a = 4;
            int b = 10;
            int c= 15;
            if(c>=a && c >= b) 
            {Console.WriteLine(c); }
            if (a >= c && a >= b)
            { Console.WriteLine(a); }
            if (b >= a && b >= c)
            { Console.WriteLine(b); }


        }
    }
}

