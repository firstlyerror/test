
using System;

namespace ConsoleApp2
{
    class Program
    {
        static void Main(string[] args)
        {
            int eded = 0, reqemsayi = 0;
            Console.Write("ededi  yazin :");
            eded = Convert.ToInt32(Console.ReadLine());
            while (eded > 0)
            {
                eded = eded / 10;
                reqemsayi++;
            }
            Console.WriteLine("ededin reqem sayi : " + reqemsayi);
            
        }
    }
}
